  "scripts": {
    "start": "pm2 start ecosystem.config.js",
    "dev": "tsc-watch --noClear --onSuccess \"npm-run-all -s production:start\"",
    "production:start": "node ./dist/src/index.js",
    "build": "rm -rf ./dist && tsc",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "watch": "pm2 delete ecosystem.config.js && pm2 start ecosystem.config.js",
    "stop": "pm2 delete ecosystem.config.js",
    "entity:create": "sh -c 'typeorm-ts-node-esm entity:create src/entities/$0'",
    "subscriber:create": "sh -c 'typeorm-ts-node-esm subscriber:create src/subscribers/$0'",
    "migrations:create": "sh -c 'typeorm-ts-node-esm migration:create src/database/migrations/$0'",
    "migrations:generate": "sh -c 'typeorm-ts-node-esm migration:generate src/migration/$0 -d src/database/connections/default.ts'",
    "migrations:run": "sh -c 'typeorm-ts-node-esm migration:run -d src/database/connections/default.ts'",
    "migrations:revert": "sh -c 'typeorm-ts-node-esm migration:revert -d src/database/connections/default.ts'",
    "migrations:show": "sh -c 'typeorm-ts-node-esm migration:show -d src/database/connections/default.ts'",
    "schema:sync": "sh -c 'rm -rf ./src/migration && mkdir ./src/migration && typeorm-ts-node-esm schema:sync -d src/database/connections/default.ts'",
    "schema:log": "sh -c 'typeorm-ts-node-esm schema:log -d src/database/connections/default.ts'",
    "schema:drop": "sh -c 'rm -rf ./src/migration && mkdir ./src/migration && typeorm-ts-node-esm schema:drop -d src/database/connections/default.ts'",
    "typeorm:db:query": "sh -c 'typeorm query $0'",
    "migrations:cache:clear": "sh -c 'typeorm-ts-node-esm cache:clear -d src/database/connections/default.ts'",
    "seed:run": "ts-node --transpile-only ./node_modules/typeorm-seeding/dist/cli.js seed -- -d src/database/connections/default.ts",
    "seed:run:single": "sh -c 'ts-node --transpile-only ./node_modules/typeorm-seeding/dist/cli.js seed --seed $0'",
    "backup:database": "mkdir -p ./db-backups && pg_dumpall --data-only --column-inserts --superuser=postgres --disable-triggers > ./db-backups/$(date +'%s').sql",
    "backup:restore": "psql -f ./db-backups/$(ls -t ./db-backups | head -n1) postgres"
  },
